{
  "$schema": "https://json.schemastore.org/package",
  "name": "simple-examples-react-memo",
  "version": "1.0.0",
  "description": "project preset with parcel bundler and react-js by Junproger",
  "keywords": [
    "parcel",
    "bundler",
    "typescript",
    "react-js",
    "preset",
    "template",
    "boilerplate"
  ],
  "homepage": "https://github.com/junproger/simple-examples-react-memo",
  "repository": "git+https://github.com/junproger/simple-examples-react-memo.git",
  "author": "Junproger <junproger@gmail.com> (https://github.com/junproger)",
  "private": true,
  "license": "ISC",
  "source": "./source/index.html",
  "targets": {
    "default": {
      "distDir": "./build"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "typing": "tsc --noEmit",
    "checkup": "prettier --check source/",
    "format": "prettier --write source/",
    "start": "parcel serve --open",
    "build": "parcel build --public-url ./",
    "watch": "parcel watch ./source/index.html",
    "linter": "eslint \"./source/**/*.{js,jsx,ts,tsx}\"",
    "linter:fix": "eslint \"./source/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.9.3",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "typescript": "^5.1.6"
  }
}
